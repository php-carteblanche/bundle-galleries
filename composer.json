{
    "name": "carte-blanche/bundle-galleries",
    "type": "carte-blanche-bundle",
    "description": "A special bundle to build SimpleViewer (http://www.simpleviewer.net/) views.",
    "keywords": [ "carte blanche", "bundles" ],
    "homepage": "http://github.com/php-carteblanche/bundle-galleries",
    "version": "0.0.1-dev",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    }],
    "require": {
        "php": ">=5.3.0",
        "atelierspierrot/patterns": "dev-master",
        "atelierspierrot/library": "dev-master",
        "carte-blanche/core": "dev-master"
    },
    "require-dev": {
        "carte-blanche/core": "dev-wip"
    },
    "autoload": { 
        "psr-0": { "SimpleViewer": "src" },
        "classmap": [ "src/SimpleViewer/Controller" ]
    },
    "repositories": [{
        "type": "composer", "url": "http://packagist.ateliers-pierrot.fr/"
    }],
    "extra": {
        "assets-dir": "www",
        "views": "src/SimpleViewer/views",
        "views-functions": "src/SimpleViewer/views_aliases.php",
        "carte-blanche-configs": "config/simpleviewer_config.ini"
    }
}
